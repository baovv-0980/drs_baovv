<%= form_for(@division, url: path) do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :parent_id %>
    <%= f.text_field :parent_id, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :parent_path %>
    <%= f.text_field :parent_path, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
