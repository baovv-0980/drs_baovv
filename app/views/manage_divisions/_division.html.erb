<tr>
  <td><%= division_counter + 1 %></td>
  <td><%= division.name %></td>
  <td><%= division.parent&.name %></td>
  <td><%= division.parent_path %></td>
  <td>
    <%= link_to manage_division_path(division),class: "my_icon" do %>
      <i class="nav-icon far fa-user"></i>
    <% end %>
    <%= link_to edit_manage_division_path(division),class: "my_icon" do %>
      <i class="nav-icon far fa-edit"></i>
    <% end %>
    <% if division.id != 1 %>
      <%= link_to manage_division_path(division), {class: "my_icon", method: :delete, data: {confirm: t(".delete_sure")}} do %>
         <i class="nav-icon far fa-trash-alt"></i>
      <% end %>
    <% end %>
  </td>
</tr>
